{%extends "NOGestionnaireBundle::layout.html.twig" %}
{% block title %}
	Modification du projet - {{ parent() }}
{% endblock %}

{% block body %}
	<div class="col-lg-3">
		<h3>Modification des jours réalisés et reste à faire</h3>
		<h4>Nom de la tâche: {{ tache.nomTache }}</h4>
		{{ form(form) }}
	</div>
	<div class="col-lg-12">
		<a href="{{ path('no_gestionnaire_suivi', {'id': tache.projet.id}) }}" class="btn btn-default"><i class="glyphicon glyphicon-chevron-left"></i>Retour au suivi</a>
	</div>
{% endblock %}